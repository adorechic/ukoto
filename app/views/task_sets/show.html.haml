- content_for :javascript do
  = javascript_pack_tag 'tasks'

%h1= @task_set.title

= react_component('TaskRoot', tasks: @task_set.tasks.map {|task| { title: task.title } }, task_set_id: @task_set.id, authenticity_token: form_authenticity_token)

%p
  = link_to 'Back', root_path

%p
  = link_to 'Delete', task_set_path(@task_set), method: :delete
